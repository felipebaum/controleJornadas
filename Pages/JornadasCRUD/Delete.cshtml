@page
@model controleJornadas.Pages.JornadasCRUD.DeleteModel

@{
    ViewData["Title"] = "Excluir Jornada";
    Layout = "_Layout";
}


<h3>Tem certeza que deseja alterar a Jornada: @Model.Jornadas.Id? </h3>
<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Jornadas.HrInicio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Jornadas.HrInicio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Jornadas.HrFim)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Jornadas.HrFim)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Jornadas.Turno)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Jornadas.Turno)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Jornadas.Valor)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Jornadas.Valor)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="Jornadas.Id" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> |
        <a asp-page="./Index">Voltar</a>
    </form>
</div>
