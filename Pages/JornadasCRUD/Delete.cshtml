@page
@model controleJornadas.Pages.JornadasCRUD.DeleteModel

@{
    ViewData["Title"] = "Excluir Jornada";
    Layout = "_Layout";
}


<h3>Tem certeza que deseja alterar a Jornada: @Model.Jornadas.Id? </h3>
<div class="row" style="justify-content: center;">
    <hr />
    <div class="col-md-4">
        <div class="form-group">
            <label asp-for="Jornadas.HrInicio" class="control-label"></label>
            <input asp-for="Jornadas.HrInicio" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label asp-for="Jornadas.HrFim" class="control-label"></label>
            <input asp-for="Jornadas.HrFim" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label asp-for="Jornadas.Turno" class="control-label"></label>
            <input asp-for="Jornadas.Turno" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label asp-for="Jornadas.Valor" class="control-label"></label>
            <input asp-for="Jornadas.Valor" class="form-control" readonly />
        </div>
        <br>
        <form method="post">
        <input type="hidden" asp-for="Jornadas.Id" />
        <div class="form-group" style="display: flex;justify-content: space-between;">
            <input type="submit" value="Deletar Jornada" class="btn btn-primary" />
            <a class="btn btn-secondary" asp-page="Index">Voltar</a>
        </div>
         </form>
    </div>

</div>