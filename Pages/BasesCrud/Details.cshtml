@page
@model controleJornadas.Pages.BasesCrud.DetailsModel

@{
    ViewData["Title"] = "Detalhes da  Filial";
    Layout = "_Layout";
}


<div class="row" style="justify-content: center;">
    <h4>Detalhes Filial @Model.Bases.Sigla</h4>
    <hr />
    <div class="col-md-4">
        <div class="form-group">
            <label asp-for="Bases.Nome" class="control-label"></label>
            <input asp-for="Bases.Nome" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label asp-for="Bases.Cidade" class="control-label"></label>
            <input asp-for="Bases.Cidade" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label asp-for="Bases.Sigla" class="control-label"></label>
            <input asp-for="Bases.Sigla" class="form-control" readonly />
        </div>
        <br>
        <div class="form-group" style="display: flex;justify-content: space-between;">
            <a class="btn btn-primary" asp-page="./Edit" asp-route-id="@Model.Bases.Id">Editar</a>
            <a class="btn btn-secondary" asp-page="./Index">Voltar</a>
        </div>
    </div>
</div>